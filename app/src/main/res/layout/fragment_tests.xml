<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameTests"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.geomhelper.Fragments.FragmentTests">

    <ScrollView
        android:id="@+id/scroll_tests"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/linearLayoutFragmentTests"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layoutAnimation="@anim/layout_animation_fall_down_courses"
            android:orientation="vertical">

        </LinearLayout>
    </ScrollView>
</FrameLayout>
